    343           * @fn      MT_UartProcessZAppData
    344           *
    345           * @brief   | SOP | CMD  |   Data Length   | FSC  |
    346           *          |  1  |  2   |       1         |  1   |
    347           *
    348           *          Parses the data and determine either is SPI or just simply serial data
    349           *          then send the data to correct place (MT or APP)
    350           *
    351           * @param   port    - UART port
    352           *          event   - Event that causes the callback
    353           *
    354           *
    355           * @return  None
    356           ***************************************************************************************************/
    357          void MT_UartProcessZAppData ( uint8 port, uint8 event )
    358          {
    359          
    360            osal_event_hdr_t  *msg_ptr;
    361            uint16 length = 0;
    362            uint16 rxBufLen  = Hal_UART_RxBufLen(MT_UART_DEFAULT_PORT);
    363          
    364            /*
    365               If maxZAppBufferLength is 0 or larger than current length
    366               the entire length of the current buffer is returned.
    367            */
    368            if ((MT_UartMaxZAppBufLen != 0) && (MT_UartMaxZAppBufLen <= rxBufLen))
    369            {
    370              length = MT_UartMaxZAppBufLen;
    371            }
    372            else
    373            {
    374              length = rxBufLen;
    375            }
    376          
    377            /* Verify events */
    378            if (event == HAL_UART_TX_FULL)
    379            {
    380              // Do something when TX if full
    381              return;
    382            }
    383          
    384            if (event & ( HAL_UART_RX_FULL | HAL_UART_RX_ABOUT_FULL | HAL_UART_RX_TIMEOUT))
    385            {
    386              if ( App_TaskID )
    387              {
    388                /*
    389                   If Application is ready to receive and there is something
    390                   in the Rx buffer then send it up
    391                */
    392                if ((MT_UartZAppRxStatus == MT_UART_ZAPP_RX_READY ) && (length != 0))
    393                {
    394                  /* Disable App flow control until it processes the current data */
    395                   MT_UartAppFlowControl (MT_UART_ZAPP_RX_NOT_READY);
    396          
    397                  /* 2 more bytes are added, 1 for CMD type, other for length */
    398                  msg_ptr = (osal_event_hdr_t *)osal_msg_allocate( length + sizeof(osal_event_hdr_t) );
    399                  if ( msg_ptr )
    400                  {
    401                    msg_ptr->event = SPI_INCOMING_ZAPP_DATA;
    402                    msg_ptr->status = length;
    403          
    404                    /* Read the data of Rx buffer */
    405                    HalUARTRead( MT_UART_DEFAULT_PORT, (uint8 *)(msg_ptr + 1), length );
    406          
    407                    /* Send the raw data to application...or where ever */
    408                    osal_msg_send( App_TaskID, (uint8 *)msg_ptr );
    409                  }
    410                }
    411              }
    412            }
    413          }
